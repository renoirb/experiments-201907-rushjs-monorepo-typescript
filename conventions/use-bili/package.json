    "@renoirb/conventions-use-prettier": "workspace:*",
    "@renoirb/conventions-use-typescript-3": "workspace:*",
    "@renoirb/tools-bundling-helpers": "workspace:*",
    "@rushstack/node-core-library": "^3.25.0",
    "@types/jest": "^25.2.3",
    "typescript": "~3.9.7",
    "@types/node": "^10.17.13"
  },
  "peerDependencies": {
    "@renoirb/tools-bundling-helpers": "1.1.2"
  }
}
