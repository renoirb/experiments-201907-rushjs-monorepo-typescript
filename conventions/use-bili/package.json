{
  "name": "@renoirb/conventions-use-bili",
  "version": "1.1.0-dev.3",
  "description": "Reusable Babel,Rollup configs leveraging https://github.com/egoist/bili",
  "homepage": "https://github.com/renoirb/experiments-201908-rush-typescript-just-bili-monorepo/tree/v1.x-dev/conventions/use-bili",
  "repository": {
    "type": "git",
    "url": "https://github.com/renoirb/experiments-201908-rush-typescript-just-bili-monorepo.git",
    "directory": "conventions/use-bili"
  },
  "license": "MIT",
  "author": {
    "name": "Renoir Boulanger",
    "email": "contribs@renoirboulanger.com"
  },
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "bin": {
    "use-bili": "./bin/bili"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "use-cross-env use-run-all clean build:*",
    "build:cjs": "use-cross-env ./bin/bili --target node --format cjs-min --input src/index.ts --file-name index.js",
    "build:esm": "use-cross-env ./bin/bili --target node --format esm-min --input src/index.ts --file-name index.esm.js",
    "clean": "use-cross-env use-rimraf .rpt2_cache dist *.*.log",
    "fix": "use-cross-env use-prettier --write '**/*.{ts,md,json,js}'",
    "prepublishOnly": "rushx build",
    "test": "use-cross-env use-jest --detectOpenHandles",
    "test:snapshots": "use-cross-env use-jest --detectOpenHandles -u"
  },
  "dependencies": {
    "@babel/core": "^7.10.0",
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/runtime-corejs3": "^7.10.0",
    "@rollup/plugin-babel": "^5.0.0",
    "bili": "^4.8.1",
    "core-js": "^3.1.4",
    "rollup": "^1.32.0",
    "rollup-plugin-typescript2": "^0.27.0"
  },
  "devDependencies": {
    "@renoirb/conventions-use-eslint": "1.1.0-dev.3",
    "@renoirb/conventions-use-jest": "1.1.0-dev.3",
    "@renoirb/conventions-use-prettier": "1.1.0-dev.3",
    "@renoirb/conventions-use-typescript-3": "1.1.0-dev.3",
    "@renoirb/tools-bundling-helpers": "1.1.0-dev.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rushstack/node-core-library": "^3.24.1",
    "@types/jest": "^25.1.3",
    "@types/node": "^10.17.13",
    "bili": "^4.8.1",
    "rollup": "^1.32.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.2.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.0"
  },
  "peerDependencies": {
    "@renoirb/tools-bundling-helpers": "1.1.0-dev.1"
  }
}
