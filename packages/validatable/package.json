{
  "name": "@renoirb/validatable",
  "version": "1.1.0-dev.3",
  "description": "Add validation rules to an object, and ability to check if a given string passes the rules",
  "repository": {
    "type": "git",
    "url": "https://github.com/renoirb/experiments-201908-rush-typescript-just-bili-monorepo.git",
    "directory": "packages/validatable"
  },
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": {
    "./dist/index.js": "./dist/browser.cjs.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "use-run-all clean build:*",
    "build:browser": "use-bili  --bundle-node-modules --target browser --format cjs --file-name browser.cjs.js",
    "build:node": "use-bili --bundle-node-modules --target node --format cjs --file-name index.js",
    "build:node-esm": "use-bili --bundle-node-modules --target node --format esm --file-name index.esm.js",
    "clean": "use-rimraf .rpt2_cache dist *.*.log",
    "fix": "use-prettier --write",
    "prepublishOnly": "rushx build",
    "test": "use-jest --detectOpenHandles",
    "test:snapshots": "use-jest --detectOpenHandles -u"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@renoirb/conventions-use-bili": "1.1.0-dev.3",
    "@renoirb/conventions-use-eslint": "1.1.0-dev.3",
    "@renoirb/conventions-use-jest": "1.1.0-dev.3",
    "@renoirb/conventions-use-prettier": "1.1.0-dev.3",
    "@renoirb/conventions-use-typescript-3": "1.1.0-dev.3",
    "@renoirb/tools-bundling-helpers": "1.1.0-dev.3",
    "@types/jest": "^25.1.3",
    "@types/node": "^10.17.13",
    "core-js": "^3.1.4",
    "tslib": "^2.0.0",
    "typescript": "^3.9.0"
  }
}
