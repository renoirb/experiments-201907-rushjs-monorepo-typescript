{
  "name": "@renoirb/validatable",
  "version": "1.1.2",
  "description": "Add validation rules to an object, and ability to check if a given string passes the rules",
  "homepage": "https://github.com/renoirb/experiments-201908-rush-typescript-just-bili-monorepo/tree/v1.x-dev/tools/bundling-helpers",
  "repository": {
    "type": "git",
    "url": "https://github.com/renoirb/experiments-201908-rush-typescript-just-bili-monorepo.git",
    "directory": "packages/validatable"
  },
  "license": "MIT",
  "author": {
    "name": "Renoir Boulanger",
    "email": "contribs@renoirboulanger.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": {
    "./dist/index.js": "./dist/browser.cjs.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "use-cross-env use-run-all clean build:*",
    "build:browser": "use-cross-env use-bili --bundle-node-modules --target browser --format cjs --file-name browser.cjs.js",
    "build:node": "use-cross-env use-bili --bundle-node-modules --target node --format cjs --file-name index.js",
    "build:node-esm": "use-cross-env use-bili --bundle-node-modules --target node --format esm --file-name index.esm.js",
    "clean": "use-cross-env use-rimraf .rpt2_cache dist *.*.log",
    "fix": "use-cross-env use-prettier --write '**/*.{ts,md,json,js}'",
    "lint": "use-cross-env use-eslint --fix '*.js'",
    "prepublishOnly": "rushx build",
    "test": "use-cross-env use-jest --detectOpenHandles",
    "test:snapshots": "use-cross-env use-jest --detectOpenHandles -u"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime-corejs3": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@renoirb/conventions-use-bili": "workspace:*",
    "@renoirb/conventions-use-eslint": "workspace:*",
    "@renoirb/conventions-use-jest": "workspace:*",
    "@renoirb/conventions-use-prettier": "workspace:*",
    "@renoirb/conventions-use-typescript-3": "workspace:*",
    "@renoirb/tools-bundling-helpers": "workspace:*",
    "@types/jest": "^25.2.3",
    "@types/node": "^10.17.13",
    "core-js": "^3.6.0"
  }
}
