{
  "name": "@renoirb/date-epoch",
  "version": "1.1.1",
  "description": "Date internal representation utilities using Numbers of seconds since January 1st 1970 (a.k.a. Epoch)",
  "homepage": "https://github.com/renoirb/experiments-201908-rush-typescript-just-bili-monorepo/tree/v1.x-dev/packages/date-epoch",
  "repository": {
    "type": "git",
    "url": "https://github.com/renoirb/experiments-201908-rush-typescript-just-bili-monorepo.git",
    "directory": "packages/date-epoch"
  },
  "license": "MIT",
  "author": {
    "name": "Renoir Boulanger",
    "email": "contribs@renoirboulanger.com"
  },
  "exports": {
    "browser": "dist/index-browser.js",
    "import": "dist/index.mjs",
    "require": "dist/index.js"
  },
  "main": "dist/index.js",
  "jsdelivr": "dist/index-browser.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "use-cross-env use-run-all clean build:*",
    "build:browser": "use-cross-env use-bili -c bili.config.ts --target browser --format umd --module-name DateEpoch --input src/index-browser.ts --file-name index-browser.js",
    "build:node": "use-cross-env use-bili -c bili.config.ts --target node --format cjs --file-name index.js",
    "build:node-mjs": "use-cross-env use-bili -c bili.config.ts --target node --format esm --file-name index.mjs",
    "clean": "use-cross-env use-rimraf .rpt2_cache dist *.*.log",
    "fix": "use-cross-env use-prettier --write '**/*.{ts,md,json,js}'",
    "lint": "use-cross-env use-eslint --fix 'src/**/*.ts'",
    "prepublishOnly": "rushx build",
    "test": "use-cross-env use-jest --detectOpenHandles",
    "test:snapshots": "use-cross-env use-jest --detectOpenHandles -u"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/runtime-corejs3": "^7.10.0",
    "@renoirb/conventions-use-bili": "1.1.0",
    "@renoirb/conventions-use-eslint": "1.1.0",
    "@renoirb/conventions-use-jest": "1.1.0",
    "@renoirb/conventions-use-prettier": "1.1.0",
    "@renoirb/conventions-use-typescript-3": "1.1.0",
    "@renoirb/tools-bundling-helpers": "1.1.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^10.17.13",
    "core-js": "^3.6.0",
    "jest": "^25.5.0",
    "jest-date-mock": "^1.0.8",
    "ts-jest": "^25.5.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.0"
  }
}
